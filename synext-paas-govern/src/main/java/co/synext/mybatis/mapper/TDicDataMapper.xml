<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.synext.mybatis.mapper.TDicDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="co.synext.mybatis.entity.TDicData">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="code" property="code" />
        <result column="comcode" property="comcode" />
        <result column="val" property="val" />
        <result column="pid" property="pid" />
        <result column="sortno" property="sortno" />
        <result column="remark" property="remark" />
        <result column="active" property="active" />
        <result column="pathkey" property="pathkey" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, code, comcode, val, pid, sortno, remark, active, pathkey
    </sql>

</mapper>
